<% if(success && success.length) { %>
<div class="alert alert-success fade show col-6 offset-3 mt-3" role="alert">
    <%= success %>
    <button type="button" class="btn-close float-end" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>

<% if(error && error.length) { %>
<div class="alert alert-danger fade show col-6 offset-3 mt-3" role="alert">
    <%= error %>
    <button type="button" class="btn-close float-end" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
<% } %>